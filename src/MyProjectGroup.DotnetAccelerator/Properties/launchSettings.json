{
  "$schema": "http://json.schemastore.org/launchsettings.json",
  "profiles": {
    "DotnetAccelerator - Production /w SQLite": {
      "commandName": "Project",
      "dotnetRunMessages": "true",
      "launchBrowser": true,
      "applicationUrl": "https://localhost:5001;http://localhost:5000;http://localhost:8090",
      "environmentVariables": {
        "PROFILES:ACTIVE": "SQLite"
      }
    },
//#if postgresql
    "DotnetAccelerator - PostgreSQL: ": {
      "commandName": "Project",
      "dotnetRunMessages": "true",
      "launchBrowser": true,
      "applicationUrl": "https://localhost:5001;http://localhost:5000;http://localhost:8090",
      "environmentVariables": {
        "ASPNETCORE_ENVIRONMENT": "Development",
        "SPRING:PROFILES:ACTIVE": "LocalPostgreSQL"
      }
    },
//#endif

//#if mysql
    "DotnetAccelerator - MySQL: ": {
      "commandName": "Project",
      "dotnetRunMessages": "true",
      "launchBrowser": true,
      "applicationUrl": "https://localhost:5001;http://localhost:5000;http://localhost:8090",
      "environmentVariables": {
        "ASPNETCORE_ENVIRONMENT": "Development",
        "SPRING:PROFILES:ACTIVE": "LocalMySQL"
      }
    },
//#endif

    "DotnetAccelerator - SqlLite: ": {
      "commandName": "Project",
      "dotnetRunMessages": "true",
      "launchBrowser": true,
      "applicationUrl": "https://localhost:5001;http://localhost:5000;http://localhost:8090",
      "environmentVariables": {
        "ASPNETCORE_ENVIRONMENT": "Development",
        "Management__Port": "5001"
      }
    }
  }
}
